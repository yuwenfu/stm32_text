N
N/* Includes ------------------------------------------------------------------*/
N#include "stm32f10x_map.h"
N
N/* Exported types ------------------------------------------------------------*/
N/* USART Init Structure definition */
Ntypedef struct
N{
N  u32 USART_BaudRate; // 设置了USART传输的波特率 ；
N  u16 USART_WordLength; // 提示了在一个帧中传输或者接收到的数据位数 ；
N  u16 USART_StopBits; // 定义了发送的停止位数目 ；
N  u16 USART_Parity;	  // 定义了奇偶模式	；
N  u16 USART_Mode;	  // 指定了使能或者失能发送和接收模式 ；
N  u16 USART_HardwareFlowControl;  // 指定了硬件流控制模式使能还是失能 ；
N} USART_InitTypeDef;
N
N/* USART Clock Init Structure definition */
Ntypedef struct
N{
N  u16 USART_Clock;  // 提示了USART时钟使能还是失能 ；
N  u16 USART_CPOL;	// 指定了下SLCK引脚上时钟输出的极性 ；
N  u16 USART_CPHA;	// 指定了下SLCK引脚上时钟输出的相位，和CPOL位一起配合来产生用户希望的时钟/数据的采样关系 ；
N  u16 USART_LastBit; //	来控制是否在同步模式下，在SCLK引脚上输出最后发送的那个数据字 (MSB)对应的时钟脉冲 ；
N} USART_ClockInitTypeDef;
N
N/* Exported constants --------------------------------------------------------*/
N#define IS_USART_ALL_PERIPH(PERIPH) (((*(u32*)&(PERIPH)) == USART1_BASE) || \
N                                     ((*(u32*)&(PERIPH)) == USART2_BASE) || \
N                                     ((*(u32*)&(PERIPH)) == USART3_BASE) || \
