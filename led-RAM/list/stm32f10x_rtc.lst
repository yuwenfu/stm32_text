N#define TIM1_OCFast_Enable                 ((u16)0x0001) //	TIM1输出比较快速特征性能使能 ；
N#define TIM1_OCFast_Disable                ((u16)0x0000) //	TIM1输出比较快速特征性能失能 ；
N
N#define IS_TIM1_OCFAST_STATE(STATE) ((STATE == TIM1_OCFast_Enable) || \
