N  assert_param(IS_DMA_ALL_PERIPH(DMAy_Channelx));
X  ((void)0);
N  assert_param(IS_DMA_CONFIG_IT(DMA_IT));
X  ((void)0);
N  assert_param(IS_FUNCTIONAL_STATE(NewState));
X  ((void)0);
N
N  if (NewState != DISABLE)
N  {
N    /* Enable the selected DMA interrupts */
