N/*******************************************************************************
N* Function Name  : DMA_Init
N* Description    : Initializes the DMAy Channelx according to the specified
N*                  parameters in the DMA_InitStruct.
N* Input          : - DMAy_Channelx: where y can be 1 or 2 to select the DMA and 
N*                    x can be 1 to 7 for DMA1 and 1 to 5 for DMA2 to select the 
N*                    DMA Channel.
N*                  - DMA_InitStruct: pointer to a DMA_InitTypeDef structure that
N*                    contains the configuration information for the specified
N*                    DMA Channel.
N* Output         : None
N* Return         : None
N* 功能           : 根据DMA_InitStruct中指定的参数初始化DMA的通道X寄存器 ；
N******************************************************************************/
Nvoid DMA_Init(DMA_Channel_TypeDef* DMAy_Channelx, DMA_InitTypeDef* DMA_InitStruct)
N{
N  u32 tmpreg = 0;
N
N  /* Check the parameters */
N  assert_param(IS_DMA_ALL_PERIPH(DMAy_Channelx));
X  ((void)0);
N  assert_param(IS_DMA_DIR(DMA_InitStruct->DMA_DIR));
X  ((void)0);
N  assert_param(IS_DMA_BUFFER_SIZE(DMA_InitStruct->DMA_BufferSize));
X  ((void)0);
N  assert_param(IS_DMA_PERIPHERAL_INC_STATE(DMA_InitStruct->DMA_PeripheralInc));
X  ((void)0);
N  assert_param(IS_DMA_MEMORY_INC_STATE(DMA_InitStruct->DMA_MemoryInc));   
X  ((void)0);   
N  assert_param(IS_DMA_PERIPHERAL_DATA_SIZE(DMA_InitStruct->DMA_PeripheralDataSize));
X  ((void)0);
N  assert_param(IS_DMA_MEMORY_DATA_SIZE(DMA_InitStruct->DMA_MemoryDataSize));
X  ((void)0);
N  assert_param(IS_DMA_MODE(DMA_InitStruct->DMA_Mode));
X  ((void)0);
N  assert_param(IS_DMA_PRIORITY(DMA_InitStruct->DMA_Priority));
X  ((void)0);
N  assert_param(IS_DMA_M2M_STATE(DMA_InitStruct->DMA_M2M));
X  ((void)0);
N
N/*--------------------------- DMAy Channelx CCR Configuration -----------------*/
N  /* Get the DMAy_Channelx CCR value */
N  tmpreg = DMAy_Channelx->CCR;
N  /* Clear MEM2MEM, PL, MSIZE, PSIZE, MINC, PINC, CIRC and DIR bits */
N  tmpreg &= CCR_CLEAR_Mask;
X  tmpreg &= ((u32)0xFFFF800F);
N  /* Configure DMAy Channelx: data transfer, data size, priority level and mode */
N  /* Set DIR bit according to DMA_DIR value */
N  /* Set CIRC bit according to DMA_Mode value */
N  /* Set PINC bit according to DMA_PeripheralInc value */
N  /* Set MINC bit according to DMA_MemoryInc value */
N  /* Set PSIZE bits according to DMA_PeripheralDataSize value */
N  /* Set MSIZE bits according to DMA_MemoryDataSize value */
N  /* Set PL bits according to DMA_Priority value */
N  /* Set the MEM2MEM bit according to DMA_M2M value */
N  tmpreg |= DMA_InitStruct->DMA_DIR | DMA_InitStruct->DMA_Mode |
N            DMA_InitStruct->DMA_PeripheralInc | DMA_InitStruct->DMA_MemoryInc |
N            DMA_InitStruct->DMA_PeripheralDataSize | DMA_InitStruct->DMA_MemoryDataSize |
N            DMA_InitStruct->DMA_Priority | DMA_InitStruct->DMA_M2M;
N  /* Write to DMAy Channelx CCR */
N  DMAy_Channelx->CCR = tmpreg;
N
N/*--------------------------- DMAy Channelx CNDTR Configuration ---------------*/
N  /* Write to DMAy Channelx CNDTR */
N  DMAy_Channelx->CNDTR = DMA_InitStruct->DMA_BufferSize;
N
N/*--------------------------- DMAy Channelx CPAR Configuration ----------------*/
N  /* Write to DMAy Channelx CPAR */
N  DMAy_Channelx->CPAR = DMA_InitStruct->DMA_PeripheralBaseAddr;
N
N/*--------------------------- DMAy Channelx CMAR Configuration ----------------*/
N  /* Write to DMAy Channelx CMAR */
N  DMAy_Channelx->CMAR = DMA_InitStruct->DMA_MemoryBaseAddr;
N}
