N/*******************************************************************************
N* Function Name  : DMA_DeInit
N* Description    : Deinitializes the DMAy Channelx registers to their default reset
N*                  values.
N* Input          : - DMAy_Channelx: where y can be 1 or 2 to select the DMA and
N*                    x can be 1 to 7 for DMA1 and 1 to 5 for DMA2 to select the 
N*                    DMA Channel.
N* Output         : None
N* Return         : None
N* 功能           : 将DMA的通道X寄存器重设为缺省值 ；
N*******************************************************************************/
Nvoid DMA_DeInit(DMA_Channel_TypeDef* DMAy_Channelx)
N{
N  /* Check the parameters */
N  assert_param(IS_DMA_ALL_PERIPH(DMAy_Channelx));
X  ((void)0);
N
N  /* Disable the selected DMAy Channelx */
N  DMAy_Channelx->CCR &= CCR_ENABLE_Reset;
X  DMAy_Channelx->CCR &= ((u32)0xFFFFFFFE);
N
N  /* Reset DMAy Channelx control register */
N  DMAy_Channelx->CCR  = 0;
N  
N  /* Reset DMAy Channelx remaining bytes register */
N  DMAy_Channelx->CNDTR = 0;
N  
N  /* Reset DMAy Channelx peripheral address register */
N  DMAy_Channelx->CPAR  = 0;
N  
N  /* Reset DMAy Channelx memory address register */
N  DMAy_Channelx->CMAR = 0;
N
N  switch (*(u32*)&DMAy_Channelx)
N  {
N    case DMA1_Channel1_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0008):
N      /* Reset interrupt pending bits for DMA1 Channel1 */
N      DMA1->IFCR |= DMA1_Channel1_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x0000000F);
N      break;
N
N    case DMA1_Channel2_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x001C):
N      /* Reset interrupt pending bits for DMA1 Channel2 */
N      DMA1->IFCR |= DMA1_Channel2_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x000000F0);
N      break;
N
N    case DMA1_Channel3_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0030):
N      /* Reset interrupt pending bits for DMA1 Channel3 */
N      DMA1->IFCR |= DMA1_Channel3_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x00000F00);
N      break;
N
N    case DMA1_Channel4_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0044):
N      /* Reset interrupt pending bits for DMA1 Channel4 */
N      DMA1->IFCR |= DMA1_Channel4_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x0000F000);
N      break;
N
N    case DMA1_Channel5_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0058):
N      /* Reset interrupt pending bits for DMA1 Channel5 */
N      DMA1->IFCR |= DMA1_Channel5_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x000F0000);
N      break;
N
N    case DMA1_Channel6_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x006C):
N      /* Reset interrupt pending bits for DMA1 Channel6 */
N      DMA1->IFCR |= DMA1_Channel6_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x00F00000);
N      break;
N
N    case DMA1_Channel7_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0080):
N      /* Reset interrupt pending bits for DMA1 Channel7 */
N      DMA1->IFCR |= DMA1_Channel7_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0000))->IFCR |= ((u32)0x0F000000);
N      break;
N
N    case DMA2_Channel1_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x0408):
N      /* Reset interrupt pending bits for DMA2 Channel1 */
N      DMA2->IFCR |= DMA2_Channel1_IT_Mask;
X      ((DMA_TypeDef *) ((((u32)0x40000000) + 0x20000) + 0x0400))->IFCR |= ((u32)0x0000000F);
N      break;
N
N    case DMA2_Channel2_BASE:
X    case ((((u32)0x40000000) + 0x20000) + 0x041C):
N      /* Reset interrupt pending bits for DMA2 Channel2 */
